<%- include("partials/header") -%>

<div>
  <h1>Home</h1>
  <p><%= startingContent %></p>
</div>


<div>
  <% newPosts.forEach((post) => { %>
    <div>
     <p>
       <%= post.title %>
       <br>
       <%= post.body.substring(0, 100) + " ..." %>
       <a href="/posts/<%= post.title %>">Read More</a>
     </p> 
    </div>
   
  <% }) %>
</div>

<%- include("partials/footer") -%>
  




