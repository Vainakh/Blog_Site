<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="POST">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" type="text" name="postTitle" id="title" autocomplete="off">
    <label for="post">Post</label>
    <textarea class="form-control" type="text" name="postBody" id="post" rows="5" cols="30"> </textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>


